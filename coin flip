function coinFlip(num){
  var tailsCount=0;
  var headsCount=0;
  for(let i = 1;i<=num;i++){
    var coin = Math.floor(Math.random()*20);
    if(coin == 1){
      headsCount++;
    }
    else{
      tailsCount++;
    }
  }
  
  return `Tails : ${tailsCount}, Heads : ${headsCount}`;
}

console.log(coinFlip(5));
